<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="SmokeSuite">

    <listeners>
        <listener class-name="com.neotech.testbase.Listener"/>
    </listeners>

    <test thread-count="5" name="SmokeTest">

        <groups>
            <run>
                <include name="smoke"/>
            </run>
        </groups>


        <packages>
            <package name="com.neotech.testcases" />
        </packages>
    </test> <!--
	SmokeTest -->
</suite> <!--
SmokeSuite -->